@model List<PreInspection>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "_layout2.cshtml";
}

<div class="container" style="margin-top:15px;margin-bottom:15px;" >
  
        @using (Html.BeginForm())
        {
            <div class="row">
  <div class="col">
     <input type="text" placeholder="ApplicationId" name="ApplicationId" class="form-control mb-2 mr-sm-2"/>
  </div>
  <div class="col">
    <button class="btn btn-primary mb-2" >Search</button>
  </div>
</div>
   
   
}

<table class="table table-responsive-sm table-bordered">
    <thead>
        <tr>
            <th>
                Type of Visit
            </th>
            <th>
                Employee Id
            </th>
            <th>
                Visit Date
            </th>
            <th>
                System Log Date
            </th>
            <th>
                Map Location
            </th>
            <th>
                
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var x in Model)
{
     <tr>
            <td>@x.Status </td>
            <td> @x.UserId</td>
            <td>@x.VisitDate</td>
            <td>@x.SystemLogDate</td>
            <td><a href="https://maps.google.com?q=@x.ClosureOfficialId" target="_blank">View Location</a></td>
            <td><a href="/Visit/ReportPreInspection/@x.Id">View Report</a></td>
        </tr>
}
    </tbody>

</table>
   
</div>

