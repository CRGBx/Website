@{
    Layout = null;

}
@if (ViewBag.Branches == null)
{
    <option value="@ViewBag.Data1" selected="selected">@ViewBag.Data1</option>
}
else
{
    foreach(BranchMaster branchMaster in (List<BranchMaster>)ViewBag.Branches)
    {
        if (branchMaster.BrCode.StartsWith("099"))
        {
            <option value="@branchMaster.BrCode" selected="selected">@branchMaster.BranchName</option>

        }
        else
        {
            <option value="@branchMaster.BrCode">@branchMaster.BranchName</option>
        }

    }
}
